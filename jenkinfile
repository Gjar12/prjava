pipeline {
agent any
environment {
  // Environment variables specified
  NAME ='Gerardo'
}

 stages {
stage('compilar') {
steps {
sh 'javac Simple.java' 
}
}
stage('ejecutar'){
steps{
sh 'java Simple.java'
}
}
stage('compilar con parametro') {
steps {
sh 'javac Param.java' 
}
}
stage('ejecutar parametro') {
steps {
sh 'java Param.java' 
}
}
}
